
<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="_HeadLinks" />
    @RenderSection("CustomCss", required: false)
</head>
<body>
    <partial name="_Loader" />
    <div id="pcoded" class="pcoded">
        <div class="pcoded-overlay-box"></div>
        <div class="pcoded-container navbar-wrapper">
            <partial name="_Header" />
            <partial name="_RightBar" />
            <div class="pcoded-main-container">
                <div class="pcoded-wrapper">
                    <partial name="_SideBar" />
                    <div class="pcoded-content">
                        <div class="pcoded-inner-content">
                            <!-- Main-body start -->
                            <div class="main-body">
                                <div class="page-wrapper">
                                     @Html.AntiForgeryToken()
                                    @RenderBody()
                                </div>
                            </div>
                            <div class="modal" tabindex="-1" role="dialog" id="form-modal">
                                <div class="modal-dialog" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title"></h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">×</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="loaderbody" id="loaderbody">
                                <div class="loader"></div>
                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </div>
    </div>
    <partial name="_FootScripts" />
    @RenderSection("ProfileJs", required: false)
    @RenderSection("CrudJs", required: false)
  
<script class="removable">
    $(".removable").remove();
    $("#searchId").on("click",function(e){
          var input =  document.querySelector("#search").value.trim();

    if (input != null && input!=''){
    console.log(input)
                $.ajax({
                type: 'post',
                url: `@Url.Action("Search","Ticket")`,
                datatype: 'json',
                data: { id:input },
                success: function (response) {
                    if (response.error == true) {

                        toastr.error(response.message, "Xəta baş verdi!");
                        return;
                    }
                },
                error: function (response) {
 
                    toastr.error("Gözlənilməz xəta baş verdi", "Xəta!");
                }
            });
          }
    })

</script>
</body>
</html>


